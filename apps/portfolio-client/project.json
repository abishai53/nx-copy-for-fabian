{
  "name": "portfolio-client",
  "projectType": "application",
  "generators": {
    "@schematics/angular:component": {
      "style": "scss",
      "skipTests": true,
      "standalone": false,
      "prefix": "ptf",
      "export": true
    }
  },
  "sourceRoot": "apps/portfolio-client/src",
  "prefix": "ptf",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/portfolio-client",
        "index": "apps/portfolio-client/src/index.html",
        "main": "apps/portfolio-client/src/main.ts",
        "polyfills": "apps/portfolio-client/src/polyfills.ts",
        "tsConfig": "apps/portfolio-client/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/portfolio-client/src/favicon.ico",
          "apps/portfolio-client/src/assets"
        ],
        "styles": [
          "apps/portfolio-client/src/styles.scss",
          "node_modules/primeicons/primeicons.css",
          "node_modules/primeng/resources/themes/vela-blue/theme.css",
          "node_modules/primeng/resources/primeng.min.css"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/portfolio-client/src/environments/environment.ts",
              "with": "apps/portfolio-client/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "portfolio-client:build:production"
        },
        "development": {
          "browserTarget": "portfolio-client:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "portfolio-client:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "apps/portfolio-client/src/test.ts",
        "polyfills": "apps/portfolio-client/src/polyfills.ts",
        "tsConfig": "apps/portfolio-client/tsconfig.spec.json",
        "karmaConfig": "apps/portfolio-client/karma.conf.js",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/portfolio-client/src/favicon.ico",
          "apps/portfolio-client/src/assets"
        ],
        "styles": [
          "apps/portfolio-client/src/styles.scss"
        ],
        "scripts": []
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/portfolio-client/**/*.ts",
          "apps/portfolio-client/**/*.html"
        ]
      }
    }
  }
}
