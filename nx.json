{
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    }
  },
  "cli": {
    "analytics": "2e9d38dd-144c-497a-8e9a-a12eb6a7cacc",
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  },
  "defaultProject": "portfolio-client"
}
